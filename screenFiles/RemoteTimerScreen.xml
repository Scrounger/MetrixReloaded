<screen name="RemoteTimerScreen" position="center,center" size="1920,1080" title="Remote Timer" flags="wfNoBorder" backgroundColor="transparent">
  <!-- Backgrounds -->
  <constant-widget name="background_window" />
  <!-- Title -->
  <!--<constant-widget name="Title_window" />-->
  <eLabel text="Timer Editor" position="window_title_pos" size="window_title_size" font="font_window_title" backgroundColor="windowBackground" transparent="1" />
  <!-- Clock -->
  <constant-widget name="clock_window" />
  <!-- Title Seperator -->
  <constant-widget name="seperator_window_title" />
  <!-- Left Panel (Left Side) -->
  <widget name="timerlist" backgroundColorRows="listBackgroundBarsOne" position="window_list_pos" size="window_list_setup_size" transparent="1" itemHeight="105" scrollbarMode="showOnDemand" backgroundColor="windowBackground" scrollbarBackgroundPicture="MetrixReloaded/scrollbar_bg.png" scrollbarWidth="6" scrollbarSliderBorderWidth="0" scrollbarKeepGapColor="1" scrollbarGap="20" />
  <!-- Right Panel (Right Side) -->
  <ePixmap alphatest="blend" pixmap="MetrixReloaded/media/icons/timer.png" position="window_image_pos" size="window_image_size" />
  <!-- Buttons -->
  <constant-widget name="color_buttons_window" />
  <constant-widget name="key_red_name" />
  <constant-widget name="key_green_name" />
  <constant-widget name="key_yellow_name" />
  <constant-widget name="key_blue_name" />
  <eLabel text="RemoteTimerScreen" name="autoGeneratedScreenName" size="200,25" font="Regular;20" backgroundColor="#2d000000" valign="center" halign="right" foregroundColor="#00ffffff" position="1720,1055" />
</screen>