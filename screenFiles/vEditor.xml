<screen name="vEditor" position="center,center" size="1920,1080" title="">
  <!-- Backgrounds -->
  <panel name="panel_background_window" />
  <!-- Title -->
  <panel name="panel_Title_window" />
  <!-- Clock -->
  <panel name="panel_clock_window" />
  <!-- Title Seperator -->
  <panel name="panel_seperator_window_title" />
  <!-- Left Panel (Left Side) -->
  <widget name="filedata" font="font_list" scrollbarMode="showOnDemand" position="window_list_pos" size="window_list_setup_size" itemHeight="40" />
  <!-- Right Panel (Right Side) -->
  <ePixmap alphatest="blend" pixmap="MetrixReloaded/media/icons/setup.png" position="window_image_pos" size="window_image_size" />
  <widget name="list_head" position="window_desc_pos" size="window_desc_size" font="font_standard" valign="center" halign="center" backgroundColor="windowBackgroundSecond" foregroundColor="foreground" transparent="1" />
  <!-- Buttons -->
  <panel name="panel_color_buttons_window" />
  <constant-widget name="key_red_name" />
  <constant-widget name="key_green_name" />
  <constant-widget name="key_yellow_name" />
  <constant-widget name="key_blue_name" />
  <widget text="vEditor" render="MetrixReloadedScreenNameLabel" size="200,25" valign="center" halign="right" font="SetrixSL;16" foregroundColor="foreground" backgroundColor="#2d000000" source="Title" position="1720,1055" />
</screen>