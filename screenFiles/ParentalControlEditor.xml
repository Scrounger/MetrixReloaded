<screen name="ParentalControlEditor" position="center,center" size="1920,1080" title="Parental control services Editor">
  <!-- Backgrounds -->
  <constant-widget name="background_window" />
  <!-- Title -->
  <!--<constant-widget name="Title_window" />-->
  <eLabel text="Parental control services Editor" position="window_title_pos" size="window_title_size" font="font_window_title" backgroundColor="windowBackground" transparent="1" />
  <!-- Clock -->
  <constant-widget name="clock_window" />
  <!-- Title Seperator -->
  <constant-widget name="seperator_window_title" />
  <!-- Left Panel (Left Side) -->
  <widget name="servicelist" position="window_list_pos" size="window_list_setup_size" textOffset="5,3" font="font_list" itemHeight="50" transparent="1" backgroundColor="windowBackground" scrollbarMode="showOnDemand" scrollbarWidth="6" scrollbarSliderBorderWidth="0" scrollbarKeepGapColor="1" scrollbarGap="20" enableWrapAround="1" />
  <!-- Right Panel (Right Side) -->
  <ePixmap alphatest="blend" pixmap="MetrixReloaded/media/icons/look.png" position="window_image_pos" size="window_image_size" />
</screen>