<screen name="TimerEntry" position="center,center" size="1920,1080" title="Timer Editor" flags="wfNoBorder" backgroundColor="transparent">
  <!-- Backgrounds -->
  <constant-widget name="background_window" />
  <!-- Title -->
  <constant-widget name="Title_window" />
  <!-- Clock -->
  <constant-widget name="clock_window" />
  <!-- Title Seperator -->
  <constant-widget name="seperator_window_title" />
  <!-- Left Panel (Left Side) -->
  <widget name="config" backgroundColorRows="listBackgroundBarsOne" position="window_list_pos" size="window_list_setup_size" textOffset="5,3" secondfont="font_list_second" font="font_list" itemHeight="50" backgroundColor="windowBackground" transparent="1" scrollbarMode="showOnDemand" scrollbarBackgroundPicture="MetrixReloaded/scrollbar_bg.png" scrollbarWidth="6" scrollbarSliderBorderWidth="0" scrollbarKeepGapColor="1" scrollbarGap="20" enableWrapAround="1" />
  <!-- Right Panel (Right Side) -->
  <ePixmap alphatest="blend" pixmap="MetrixReloaded/media/icons/timer.png" position="window_image_pos" size="window_image_size" />
  <widget source="configdesc" render="Label" foregroundColor="foreground" font="font_standard" size="window_desc_size" position="window_desc_pos" transparent="1" backgroundColor="windowBackgroundSecond" />
  <!-- Buttons -->
  <constant-widget name="color_buttons_window" />
  <widget name="canceltext" position="key_red_pos" zPosition="1" size="key_size" font="font_key_standard" halign="left" valign="top" backgroundColor="windowMainColorH" transparent="1" />
  <widget name="oktext" position="key_green_pos" zPosition="1" size="key_size" font="font_key_standard" halign="left" valign="top" backgroundColor="windowMainColorH" transparent="1" />
  <eLabel text="TimerEntry" name="autoGeneratedScreenName" size="200,25" font="Regular;20" backgroundColor="#2d000000" valign="center" halign="right" foregroundColor="#00ffffff" position="1720,1055" />
</screen>