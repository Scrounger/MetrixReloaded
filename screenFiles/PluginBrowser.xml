<screen name="PluginBrowser" position="center,center" size="1920,1080" title="Plugin browser" flags="wfNoBorder" backgroundColor="transparent">
  <!-- Backgrounds -->
  <constant-widget name="background_window" />
  <!-- Title -->
  <constant-widget name="Title_window" />
  <!-- Clock -->
  <constant-widget name="clock_window" />
  <!-- Title Seperator -->
  <constant-widget name="seperator_window_title" />
  <!-- Left Panel (Left Side) -->
  <widget name="list" backgroundColorRows="listBackgroundBarsOne" itemHeight="82" position="window_list_pos" size="window_list_setup_two_rows_size" transparent="1" backgroundColor="windowBackground" scrollbarMode="showOnDemand" scrollbarBackgroundPicture="MetrixReloaded/media/scrollbar_bg.png" scrollbarWidth="6" scrollbarSliderBorderWidth="0" scrollbarKeepGapColor="1" scrollbarGap="20" enableWrapAround="1" />
  <!-- Right Panel (Right Side) -->
  <ePixmap alphatest="blend" pixmap="MetrixReloaded/media/icons/plugin.png" position="window_image_pos" size="window_image_size" />
  <!-- Buttons -->
  <constant-widget name="color_buttons_window" />
  <constant-widget name="red_name" />
  <constant-widget name="green_name" />
  <constant-widget name="yellow_name" />
  <constant-widget name="blue_name" />
  <widget text="PluginBrowser" render="MetrixReloadedScreenNameLabel" size="200,25" valign="center" halign="right" font="SetrixSL;16" foregroundColor="foreground" backgroundColor="#2d000000" source="Title" position="1720,1055" />
</screen>