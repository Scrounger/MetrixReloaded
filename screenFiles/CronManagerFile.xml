<screen name="CronManagerFile" position="center,center" size="1920,1080" title="DVD File Browser" flags="wfNoBorder" backgroundColor="transparent">
  <!-- Backgrounds -->
  <constant-widget name="background_window" />
  <!-- Title -->
  <constant-widget name="Title_window" />
  <!-- Clock -->
  <constant-widget name="clock_window" />
  <!-- Title Seperator -->
  <constant-widget name="seperator_window_title" />
  <!-- Left Panel (Left Side) -->
  <widget name="filelist" position="window_list_pos" size="window_list_setup_size" itemHeight="50" transparent="1" scrollbarMode="showOnDemand" scrollbarWidth="6" scrollbarSliderBorderWidth="0" scrollbarGap="20" scrollbarKeepGapColor="1" />
  <!-- Right Panel (Right Side) -->
  <ePixmap alphatest="blend" pixmap="MetrixReloaded/media/icons/vti.png" position="window_image_pos" size="window_image_size" />
  <widget name="executable" position="window_desc_pos" size="window_desc_size" font="font_standard" foregroundColor="foreground" backgroundColor="windowBackground" transparent="1" />
  <!-- Buttons -->
  <constant-widget name="color_buttons_window" />
  <constant-widget name="key_red_source" />
  <constant-widget name="key_green_source" />
  <widget name="keyblue" position="key_blue_mid_pos" zPosition="1" size="key_size_mid" font="font_key_standard" halign="left" valign="top" backgroundColor="windowBackground" transparent="1" />
  <widget text="CronManagerFile" render="MetrixReloadedScreenNameLabel" size="200,25" valign="center" halign="right" font="SetrixSL;16" foregroundColor="foreground" backgroundColor="#2d000000" source="global.CurrentTime" position="1720,1055" />
</screen>