<screen name="picshow" position="center,center" size="1920,1080" title="PicturePlayer" flags="wfNoBorder" backgroundColor="transparent">
  <!-- Backgrounds -->
  <constant-widget name="background_window" />
  <!-- Title -->
  <constant-widget name="Title_window" />
  <!-- Clock -->
  <constant-widget name="clock_window" />
  <!-- Title Seperator -->
  <constant-widget name="seperator_window_title" />
  <!-- Left Panel (Left Side) -->
  <widget name="filelist" position="window_list_pos" size="window_list_setup_size" itemHeight="50" transparent="1" font="font_list" scrollbarMode="showOnDemand" scrollbarWidth="6" scrollbarSliderBorderWidth="0" scrollbarGap="20" scrollbarKeepGapColor="1" />
  <!-- Right Panel (Right Side) -->
  <ePixmap alphatest="blend" pixmap="MetrixReloaded/media/icons/camera.png" position="window_image_pos" size="window_image_size" />
  <widget name="thn" position="window_desc_pos" size="window_desc_size" alphatest="on" />
  <!-- Buttons -->
  <constant-widget name="color_buttons_window" />
  <constant-widget name="key_red_source" />
  <constant-widget name="key_green_source" />
  <constant-widget name="key_yellow_source" />
  <constant-widget name="key_blue_source" />
  <eLabel text="picshow" name="autoGeneratedScreenName" size="200,25" font="Regular;20" backgroundColor="#2d000000" valign="center" halign="right" foregroundColor="#00ffffff" position="1720,1055" />
</screen>